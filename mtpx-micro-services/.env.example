# Conex√£o com o linkd (TCP via Docker)
LINKD_CONNECT=tcp://localhost:9999
LINKD_TCP_TOKEN=<gerado pelo install.sh>

LINKD_NAMESPACE=microservice-demo
LINKD_KEYSTORE_NAMESPACE=default

# OIDC provider stored in linkd keystore (service_type/name).
# Supported:
#   oidc/<name>
#   keycloak/<name>
#   <name> (alias for oidc/<name>)
AUTH_PROVIDER=oidc/default
AUTH_REALM=multpex
AUTH_CLIENT_ID=multpex-services

# Multi-tenant migration selector (mtpx db:migrate --all-tenants)
MTPX_DB_SERVER=local-pg
MTPX_TENANT_DATABASES_INCLUDE=local-pg-*
MTPX_TENANT_DATABASES_EXCLUDE=

# DEBUG=true
NODE_ENV=development
